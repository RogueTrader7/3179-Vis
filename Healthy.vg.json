{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 300,
    "title": "Healthy person by State in Australia 2019",
    "projection":{"type": "equirectangular"},
    "data":{
        "url": "https://github.com/RogueTrader7/3179-Vis/blob/main/STE11aAust.topojson",
          "format":{"type":"topojson", "feature": "STE11aAust"}
    },
    "transform": [
        {
            "lookup": "properties.State",
            "from": {
                "data": {
                    "url": "https://raw.githubusercontent.com/RogueTrader7/3179-Vis/main/Physical%20summary_%20by%20region.csv"
        },
            "key": "State",
            "fields": ["2019"]
      }
        },
        {"calculate": "datum.Active + 0.1", "as": "population"}
    ],
    "mark":{"type": "geoshape"},
    "encoding": {
        "color":{
            "field":"2019",
            "type": "quantitative",
            "scale": {"domain":[10,50]}
        },
        "tooltip":[
            {"field":"properties.State","type": "nominal","title":"State"},
            {"field":"2019","type": "quantitative"}
        ]
    
    }
}
